@model MvcMusicStore.Models.Genre
@{
    ViewBag.Title = "Browse";
}

<h2>Browsing Genre: @Model.GenreName</h2>

<ul>
    @foreach (var album in @Model.Albums)
    {
        @*
            <li>
                @album.Title
            </li>
        *@
        <li>
            @Html.ActionLink(album.Title, "Details", new
            {
                id =
                album.Title
            })
        </li>

    }
</ul>


